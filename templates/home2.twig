<!doctype html>
<html lang="en">
    {% include 'partials/head.twig' %}
    <body >
    {% include 'partials/navigation.twig' %}
        <main class="container-fluid" style="padding-top:30px;">
                    <div class="carousel slide" id="product-carousel" data-ride="carousel">
                        <div class="carousel-inner">
                        {% for image in images %}
                        {% set active = loop.index0 == 0 ? 'active' : '' %}
                            <div class="carousel-item {{active}}">
                                <img class="w-100" src="images/promotion/{{image}}">
                            </div>
                        {% endfor %}
                        </div>
                        <ol class="carousel-indicators">
                            {% for image in images %}
                            <li data-target="#product-carousel" data-slide-to="{{loop.index0}}">
                            </li>
                            {% endfor %}
                        </ol>
                        <a class="carousel-control-prev" href="#product-carousel" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#product-carousel" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                   
                </div>
            </div>
        </main>
          <div>
         {% include 'partials/footer.twig' %}
        </div>
    </body>
</html>